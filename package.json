{
  "name": "personal-cooking-recipe",
  "version": "1.0.0",
  "description": "Personal cooking recipe management system",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "claude-start": "node /mnt/Linux-ExHDD/Claude-GlobalSettings/scripts/start-integrated.js",
    "claude-flow": "npx claude-flow@alpha swarm --claude",
    "claude-status": "node /mnt/Linux-ExHDD/Claude-GlobalSettings/scripts/system-status.js"
  },
  "keywords": [
    "recipe",
    "cooking",
    "management"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}