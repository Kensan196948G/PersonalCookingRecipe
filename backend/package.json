{
  "name": "personal-cooking-recipe-backend",
  "version": "1.0.0",
  "description": "Backend API server for Personal Cooking Recipe",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage --passWithNoTests",
    "test:unit": "jest --testMatch='**/*.test.js' --coverage --passWithNoTests",
    "test:integration": "jest --testMatch='**/*.integration.test.js' --coverage --passWithNoTests",
    "test:performance": "node ../scripts/simple-performance-test.js",
    "lint": "eslint src/ --ext .js",
    "db:migrate": "node src/config/migrate.js"
  },
  "keywords": [
    "recipe",
    "api",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "redis": "^4.6.10",
    "express-session": "^1.17.3",
    "connect-sqlite3": "^0.9.13",
    "ejs": "^3.1.9",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0"
  }
}
