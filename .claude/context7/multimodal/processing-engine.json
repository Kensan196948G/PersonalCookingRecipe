{
  "multimodal_processing": {
    "version": "2.0.0",
    "architecture": "plugin_based_pipeline",
    "processors": {
      "text": {
        "engine": "claude-3.5-sonnet",
        "capabilities": [
          "code_analysis",
          "documentation_parsing",
          "log_analysis",
          "natural_language_processing"
        ],
        "formats": [
          "javascript", "typescript", "json", "yaml", "markdown",
          "sql", "dockerfile", "nginx.conf", "env"
        ],
        "analysis_types": [
          "syntax_analysis",
          "semantic_analysis", 
          "complexity_metrics",
          "security_patterns",
          "performance_patterns"
        ]
      },
      "image": {
        "engine": "claude-3.5-sonnet-vision",
        "capabilities": [
          "ui_screenshot_analysis",
          "error_screenshot_parsing",
          "architecture_diagram_reading",
          "code_diagram_interpretation"
        ],
        "formats": ["png", "jpg", "jpeg", "svg", "webp", "gif"],
        "analysis_types": [
          "ui_component_detection",
          "error_message_extraction",
          "layout_analysis",
          "accessibility_review"
        ]
      },
      "audio": {
        "engine": "whisper-large-v3",
        "capabilities": [
          "meeting_transcription",
          "voice_memo_processing",
          "requirement_gathering",
          "stakeholder_interviews"
        ],
        "formats": ["mp3", "wav", "m4a", "flac"],
        "analysis_types": [
          "speech_to_text",
          "sentiment_analysis",
          "speaker_identification",
          "action_item_extraction"
        ]
      },
      "video": {
        "engine": "claude-3.5-sonnet-vision + whisper",
        "capabilities": [
          "demo_video_analysis",
          "tutorial_processing",
          "code_review_videos",
          "user_testing_sessions"
        ],
        "formats": ["mp4", "webm", "avi", "mov"],
        "analysis_types": [
          "frame_by_frame_analysis",
          "audio_transcript_sync",
          "interaction_pattern_detection",
          "performance_bottleneck_identification"
        ]
      }
    },
    "integration": {
      "context7_layers": {
        "layer1": "project_documentation_analysis",
        "layer2": "code_screenshot_analysis", 
        "layer3": "database_diagram_processing",
        "layer4": "meeting_audio_transcription",
        "layer5": "performance_video_analysis",
        "layer6": "test_result_screenshot_parsing",
        "layer7": "deployment_log_analysis"
      },
      "agent_coordination": {
        "recipe_cto": "architectural_decision_videos",
        "recipe_dev": "code_review_screenshots",
        "recipe_security": "security_audit_reports",
        "recipe_performance": "performance_charts",
        "recipe_devops": "infrastructure_diagrams"
      }
    },
    "processing_pipeline": {
      "input_validation": {
        "file_type_detection": "magic_number_based",
        "malware_scanning": "clamav",
        "size_limits": {
          "image": "50MB",
          "audio": "500MB", 
          "video": "2GB"
        }
      },
      "preprocessing": {
        "image": ["format_normalization", "compression", "metadata_extraction"],
        "audio": ["noise_reduction", "format_conversion", "segmentation"],
        "video": ["frame_extraction", "audio_separation", "compression"]
      },
      "analysis": {
        "parallel_processing": true,
        "batch_optimization": true,
        "caching": "aggressive",
        "error_handling": "graceful_degradation"
      },
      "output_integration": {
        "context7_storage": "automatic",
        "agent_notification": "real_time",
        "user_feedback": "interactive"
      }
    }
  }
}